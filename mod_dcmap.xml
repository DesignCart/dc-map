<?xml version="1.0" encoding="utf-8"?><extension type="module" version="5.0" client="site" method="upgrade">	<name>MOD_DCMAP</name>	<author>Design Cart</author>	<version>1.0.0</version>	<description>The module displays awesome Google Maps with custom styles.</description>	<copyright>Copyright (C) 2011-2016 Design Studio WWW. All rights reserved.</copyright>	<license>http://www.gnu.org/licenses/gpl-3.0.html</license>	<updateservers>		<server type="extension" name="MOD_DCMAP Update Server">https://www.designcart.pl/download/updates/update_mod_dcmap.xml</server>	</updateservers>	<files>		<filename module="mod_dcmap">mod_dcmap.php</filename>		<filename>helper.php</filename>		<folder>tmpl</folder>		<filename>index.html</filename>	</files>	<languages>		<language tag="en-GB">language/en-GB/en-GB.mod_dcmap.ini</language>		<language tag="en-GB">language/en-GB/en-GB.mod_dcmap.sys.ini</language>		<language tag="pl-PL">language/pl-PL/pl-PL.mod_dcmap.ini</language>		<language tag="pl-PL">language/pl-PL/pl-PL.mod_dcmap.sys.ini</language>	</languages>	<config>		<fields name="params">			<fieldset name="basic" label="MOD_DCMAP_FIELDSET_BASIC">				<field name="Marker" type="media" label="GOOGLEMAP_MARKER" />				<field name="Zoom" type="list" default="14" label="GOOGLEMAP_ZOOM">					<option value="1">1</option>					<option value="2">2</option>					<option value="3">3</option>					<option value="4">4</option>					<option value="5">5</option>					<option value="6">6</option>					<option value="7">7</option>					<option value="8">8</option>					<option value="9">9</option>					<option value="10">10</option>					<option value="11">11</option>					<option value="12">12</option>					<option value="13">13</option>					<option value="14">14</option> 					<option value="15">15</option>					<option value="16">16</option>					<option value="17">17</option>					<option value="18">18</option>					<option value="19">19</option>					<option value="20">20</option>				</field>				<field name="Latitude" type="text" label="GOOGLEMAP_MARKER_LATITUDE" filter="string" />				<field name="Longitude" type="text" label="GOOGLEMAP_MARKER_LONGITUDE" filter="string" />				<field name="Height" type="text" default="400px" label="GOOGLEMAP_MARKER_HEIGHT" />				<field name="Width" type="text" default="100%" label="GOOGLEMAP_MARKER_WIDTH" />				<field name="Style" type="textarea" label="GOOGLEMAP_STYLE" filter="string" />				<field name="UseAPI" type="radio" default="1" label="GOOGLEMAP_USEAPI">					<option value="0">JNO</option>					<option value="1">JYES</option>				</field>				<field name="ApiKey" type="text" label="GOOGLEMAP_APIKEY" />			</fieldset>		</fields>	</config></extension>